{
    "rewrites": [
        {
            "source": "/api/admin/(.*)",
            "destination": "https://andre23arruda.pythonanywhere.com/admin/$1"
        },
        {
            "source": "/api/admin",
            "destination": "https://andre23arruda.pythonanywhere.com/admin/"
        },
        {
            "source": "/api/:path*",
            "destination": "https://andre23arruda.pythonanywhere.com/:path*"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/"
        }
    ]
}